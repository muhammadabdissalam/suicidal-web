{
  "name": "nuxt3-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "nuxt": "nuxt",
    "reset": "nuxt cleanup && nuxt prepare",
    "typecheck": "nuxt typecheck",
    "build": "nuxt build",
    "dev": "nuxt dev --clipboard --no-clear",
    "devsecure": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:prettier": "prettier --write --list-different .",
    "lintfix": "npm run lintfix:js && npm run lintfix:prettier",
    "packages:check": "npx npm-check-updates",
    "packages:update": "npx npm-check-updates --interactive --format group"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{css,md,json}": "prettier --write",
    "*": "secretlint"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=20",
    "npm": ">=10.2"
  },
  "overrides": {
    "vue": "latest",
    "@babel/traverse": "7.23.2"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.14",
    "@nuxtjs/fontaine": "0.4.1",
    "@nuxtjs/google-fonts": "3.0.2",
    "@nuxtjs/i18n": "8.0.0-rc.4",
    "@pinia/nuxt": "0.5.1",
    "@unocss/preset-mini": "0.58.0",
    "@vee-validate/nuxt": "4.11.8",
    "@vee-validate/yup": "4.12.5",
    "@vueuse/core": "10.6.1",
    "@vueuse/nuxt": "10.6.1",
    "class-variance-authority": "0.7.0",
    "dayjs": "1.11.10",
    "jw-paginate": "1.0.4",
    "keen-slider": "6.8.6",
    "lodash-es": "4.17.21",
    "nuxt": "3.10.2",
    "nuxt-icon": "0.6.6",
    "nuxt-security": "0.14.4",
    "pinia": "2.1.7",
    "remark": "14.0.3",
    "remark-gfm": "3.0.1",
    "remark-html": "15.0.2",
    "vee-validate": "4.11.8",
    "vite-plugin-compression2": "0.12.0",
    "vue-html-secure": "1.0.10",
    "vue-tippy": "6.4.1",
    "webrtc-adapter": "7.5.0",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@nuxt/image": "1.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@secretlint/secretlint-rule-preset-recommend": "^7.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.4",
    "autoprefixer": "10.4.19",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-clean-timer": "^1.0.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-security-node": "^1.1.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "lint-staged": "^15.1.0",
    "postcss": "8.4.38",
    "prettier": "^3.1.0",
    "secretlint": "^7.1.0",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "3.4.1",
    "typescript": "^5.3.2",
    "vue-tsc": "^1.8.22"
  }
}